{
  "name": "hyde-cms",
  "version": "0.1.0",
  "description": "CMS for fast blog building with Jekyll",
  "main": "bundle.js",
  "scripts": {
    "build": "webpack --colors --progress",
    "run": "webpack-dev-server --watch",
    "test": "jest --verbose --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abac00s/HydeCMS.git"
  },
  "author": "Adam Bac",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/abac00s/HydeCMS/issues"
  },
  "homepage": "https://github.com/abac00s/HydeCMS#readme",
  "dependencies": {
    "history": "^2.0.1",
    "isomorphic-fetch": "^2.2.1",
    "js-yaml": "^3.5.5",
    "markdown": "^0.5.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.0",
    "react-tinymce": "^0.4.0",
    "redux": "^3.3.1",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-jest": "^10.0.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-async-to-generator": "^6.7.4",
    "babel-plugin-transform-regenerator": "^6.6.5",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "jest-cli": "^0.10.0",
    "react-addons-test-utils": "^0.14.8",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  }
}
